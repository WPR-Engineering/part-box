<p id="notice"><%= notice %></p>

<h1>Consumables</h1>

<table>
  <thead>
    <tr>

      <th>Name</th>
      <th>Description</th>
      <th>Quantity</th>
      <th>Reorderamount</th>

      <th>Shelf</th>
      <th>Bin</th>
      <th>Obsolete</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumables.each do |consumable| %>
      <tr>

        <td><%= consumable.name %></td>
        <td><%= consumable.description %></td>
        <td><%= consumable.quantity %></td>
        <td><%= consumable.reorderAmount %></td>

        <td><%= consumable.shelf %></td>
        <td><%= consumable.bin %></td>
        <td><%= consumable.obsolete %></td>
        <td><%= link_to 'Show', consumable %></td>
        <td><%= link_to 'Edit', edit_consumable_path(consumable) %></td>
        <td><%= link_to 'Destroy', consumable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consumable', new_consumable_path %>
